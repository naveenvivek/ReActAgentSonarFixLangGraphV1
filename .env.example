# .env.example - Configuration Template
# Copy this file to .env and update with your actual values

# SonarQube Configuration
SONAR_URL=http://localhost:9100
SONAR_TOKEN=your_sonar_token_here
SONAR_PROJECT_KEY=your_project_key_here

# Target Application Repository  
TARGET_REPO_URL=https://github.com/your-username/your-repo
TARGET_REPO_PATH=/path/to/your/local/repo
TARGET_REPO_BRANCH=main

# AI Configuration
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1:latest

# Observability
LANGFUSE_URL=http://localhost:3000
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key-here
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key-here

# Git Configuration
GIT_USER_NAME=SonarQube AI Agent
GIT_USER_EMAIL=your-email@example.com
MR_TARGET_BRANCH=main

# GitHub Configuration
GITHUB_TOKEN=your_github_token_here
GITHUB_USERNAME=your_username